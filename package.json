    "bugs": {
        "url": "https://github.com/geneseframework/api/issues"
    },
    "homepage": "https://github.com/geneseframework/api#readme",
    "dependencies": {
        "@types/node": "^14.14.8",
        "chalk": "^4.1.0",
        "eol": "^0.9.1",
        "fs-extra": "^9.0.1",
        "handlebars": "^4.7.6",
        "ora": "^5.1.0"
    }
